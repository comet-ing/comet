{
    "name": "jam-twt",
    "version": "0.1.0",
    "description": "JAM-TWT monorepo",
    "repository": "git@github.com:riseandshaheen/jam-twt.git",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --no-cache --continue",
        "lint": "turbo run lint",
        "clean": "turbo run clean && rm -rf node_modules",
        "format:check": "prettier \"**/*.{ts,tsx, js}\" --check",
        "format": "prettier --write \"**/*.{js,ts,tsx,md}\""
    },
    "devDependencies": {
        "prettier": "3.0.3",
        "turbo": "^1.13.3"
    },
    "engines": {
        "node": ">=20.10.0"
    },
    "packageManager": "yarn@1.22.19"
}
